        <html>
            <head>
                <meta charset="utf-8">
            </head>
            <body>
                <div> <strong>Список гостниц</strong> </div>
                <div>
                    <table>
                    {% for name in hotels_list %}
                        {% set splitted = name.split(',') %}
                        <tr>
                            <td>{{splitted[1]}}</td><td>{{ splitted[4]}}</td><td>{{splitted[5]}}</td><td>{{splitted[6]}}</td>
                            <td>
                                <form method="POST" action="./hotels.py" style="display: inline-block">
                                    <input type='hidden' name='uuid' value='{{splitted[0]}}'></input>
                                    <button type= 'submit'>Удалить</button>
                                </form>
                            </td>
                        </tr>
                    {% endfor %}
                    </table>
                </div>
            </body>
        <html>